{
    "spec": {
        "listeners": [
            {
                "healthCheck": {
                    "healthyThreshold": 2,
                    "intervalMillis": 5000,
                    "port": 8080,
                    "path": "/ping",
                    "protocol": "http2",
                    "timeoutMillis": 2000,
                    "unhealthyThreshold": 3
                },
                "portMapping": {
                    "port": 8080,
                    "protocol": "http2"
                }
            }
        ],
        "serviceDiscovery": {
            "awsCloudMap": {
                "attributes": [
                  {
                    "key": "ECS_TASK_DEFINITION_FAMILY",
                    "value": "red"
                  }
                ],
                "namespaceName": "http2.local",
                "serviceName": "color_server"
            }
        }
    },
    "virtualNodeName": "color_server-red"
}
