{
    "spec": {
        "listeners": [
            {
                "healthCheck": {
                    "healthyThreshold": 2,
                    "intervalMillis": 5000,
                    "path": "/ping",
                    "protocol": "http2",
                    "timeoutMillis": 2000,
                    "unhealthyThreshold": 3
                },
                "portMapping": {
                    "port": 8080,
                    "protocol": "http2"
                }
            }
        ],
        "serviceDiscovery": {
            "awsCloudMap": {
                "attributes": [
                    {
                        "key": "ECS_TASK_DEFINITION_FAMILY",
                        "value": "red"
                    }
                ],
                "namespaceName": "default.svc.cluster.local",
                "serviceName": "color-server-red",
                "ipPreference": "IPv4_ONLY"
            }
        }
    },
    "virtualNodeName": "color_server-red"
}