{
        "meshName": "appmesh-mesh-logging",
        "spec": {
            "backends": [],
            "listeners": [
                {
                    "healthCheck": {
                        "healthyThreshold": 2,
                        "intervalMillis": 5000,
                        "path": "/ping",
                        "port": 9080,
                        "protocol": "http",
                        "timeoutMillis": 2000,
                        "unhealthyThreshold": 2
                    },
                    "portMapping": {
                        "port": 9080,
                        "protocol": "http"
                    }
                }
            ],
            "logging": {
                "accessLog": {
                    "file": {
                        "path": "/dev/stdout"
                    }
                }
            },
            "serviceDiscovery": {
                "dns": {
                    "hostname": "colorteller-green.logging.local"
                }
            }
        },
        "virtualNodeName": "colorteller-green-vn"
}
