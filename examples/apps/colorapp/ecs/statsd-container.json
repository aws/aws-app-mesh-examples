{
  "name": "statsd",
  "image": $STATSD_IMAGE,
  "user": "1337",
  "essential": true,
  "portMappings": [
    {
      "containerPort": 8125,
      "protocol": "udp"
    }
  ],
  "environment": [
    {
      "name": "AWS_REGION",
      "value": $AWS_REGION
    },
    {
      "name": "CLOUDWATCH_NAMESPACE",
      "value": "ColorApp"
    }
  ],
  "logConfiguration": {
    "logDriver": "awslogs",
    "options": {
      "awslogs-group": $ECS_SERVICE_LOG_GROUP,
      "awslogs-region": $AWS_REGION,
      "awslogs-stream-prefix": $AWS_LOG_STREAM_PREFIX
    }
  }
}